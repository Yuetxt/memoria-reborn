{
  "compilerOptions": {
    "target": "es2021",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": false,
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist",
    "rootDir": "src",

  },
  "include": ["src/**/*"],
  "exclude": ["commands/**/*"]
}
